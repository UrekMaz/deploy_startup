{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
        
      }],
      "rewrites": [
    { "source": "/investor_profile/:userId", "destination": "/index.html" },
    { "source": "/startup_profile/:userId", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
    
}

