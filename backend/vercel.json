{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
        
      }],
      "routes": [
        { "src": "/investor_profile/([^/]+)", "dest": "/index.html" },
        { "src": "/startup_profile/([^/]+)", "dest": "/index.html" },
        {
          "src": "/(.*)",
          "dest": "/index.js"
        }
      ]
    
}

